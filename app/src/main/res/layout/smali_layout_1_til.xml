<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="match_parent"
	android:layout_width="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical">

		<LinearLayout
			android:layout_height="match_parent"
			android:layout_width="match_parent"
			android:orientation="horizontal"
			android:layout_weight="1.0">

			<TextView
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				android:textSize="14sp"
				android:text="百度搜索生成"
				android:id="@+id/smalilayout1tilButton1"
				android:gravity="center"
				android:layout_weight="1.0"
				android:background="@drawable/tb_"/>

			<TextView
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				android:textSize="14sp"
				android:background="@drawable/tb_"
				android:text="菜谱查询"
				android:id="@+id/smalilayout1tilButton2"
				android:gravity="center"
				android:layout_weight="1.0"/>

			<TextView
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				android:textSize="14sp"
				android:background="@drawable/tb_"
				android:text="邮编查询"
				android:id="@+id/smalilayout1tilButton3"
				android:gravity="center"
				android:layout_weight="1.0"/>

		</LinearLayout>

		<LinearLayout
			android:layout_height="match_parent"
			android:layout_width="match_parent"
			android:orientation="horizontal"
			android:layout_weight="1.0">

			<TextView
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				android:textSize="14sp"
				android:text="手机号码查询"
				android:layout_weight="1.0"
				android:background="@drawable/tb_"
				android:gravity="center"
				android:id="@+id/smalilayout1tilButton4"/>

			<TextView
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				android:textSize="14sp"
				android:gravity="center"
				android:background="@drawable/tb_"
				android:layout_weight="1.0"
				android:text="身份证信息查询"
				android:id="@+id/smalilayout1tilButton5"/>

			<TextView
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				android:textSize="14sp"
				android:gravity="center"
				android:background="@drawable/tb_"
				android:text="手机号码查吉凶"
				android:layout_weight="1.0"
				android:id="@+id/smalilayout1tilButton6"/>

		</LinearLayout>

		<LinearLayout
			android:layout_height="match_parent"
			android:layout_width="match_parent"
			android:orientation="horizontal"
			android:layout_weight="1.0">

			<TextView
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				android:textSize="14sp"
				android:gravity="center"
				android:background="@drawable/tb_"
				android:text="银行卡信息查询"
				android:layout_weight="1.0"
				android:id="@+id/smalilayout1tilButton7"/>

			<TextView
				android:layout_height="match_parent"
				android:layout_weight="1.0"
				android:layout_width="match_parent"
				android:textSize="14sp"
				android:gravity="center"
				android:background="@drawable/tb_"
