				android:gravity="center"
				android:textSize="14sp"
				android:background="@drawable/tb_"
				android:text="各省今日油价"
				android:id="@+id/smalilayout1tilButton17"/>

			<TextView
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				android:textSize="14sp"
				android:layout_weight="1.0"
				android:background="@drawable/tb_"
				android:gravity="center"
				android:text="中国彩票开奖结果"
				android:id="@+id/smalilayout1tilButton18"/>

		</LinearLayout>

		<LinearLayout
			android:layout_height="match_parent"
			android:layout_width="match_parent"
			android:orientation="horizontal"
			android:layout_weight="1.0">

			<TextView
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				android:layout_weight="1.0"
				android:background="@drawable/tb_"
				android:textSize="14sp"
				android:gravity="center"
				android:text="黄金数据查询"
				android:id="@+id/smalilayout1tilButton21"/>

			<TextView
				android:layout_height="match_parent"
				android:layout_weight="1.0"
				android:layout_width="match_parent"
				android:textSize="14sp"
				android:background="@drawable/tb_"
				android:gravity="center"
				android:text="汇率数据查询"
				android:id="@+id/smalilayout1tilButton22"/>

			<TextView
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				android:gravity="center"
				android:background="@drawable/tb_"
				android:textSize="14sp"
				android:text="天气预报查询"
				android:id="@+id/smalilayout1tilButton23"
				android:layout_weight="1.0"/>

		</LinearLayout>

		<LinearLayout
			android:layout_height="match_parent"
			android:layout_width="match_parent"
			android:orientation="horizontal"
			android:layout_weight="1.0">

			<TextView
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				android:textSize="14sp"
				android:gravity="center"
				android:background="@drawable/tb_"
				android:text="微信精选查询"
				android:layout_weight="1.0"
				android:id="@+id/smalilayout1tilButton19"/>

			<TextView
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				android:textSize="14sp"
				android:background="@drawable/tb_"
				android:layout_weight="1.0"
				android:gravity="center"
				android:text="票房查询"
				android:id="@+id/smalilayout1tilButton20"/>

		</LinearLayout>

	</LinearLayout>

</RelativeLayout>

